{
    "name": "underwear/php-eventerra-sdk",
    "description": "Not official Eventerra API SDK for PHP",
    "keywords": ["eventerra", "sdk"],
    "type": "library",
    "homepage": "https://github.com/underwear/php-eventerra-sdk",
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0",
        "php-http/message": "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/discovery": "^1.4"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.0",
        "monolog/monolog": "^1.11"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Guzzle. Awesome HTTP client",
        "monolog/monolog": "PSR-3 Logger. Allows to log all the API requests and responses"
    },
    "autoload": {
        "psr-4": {
            "Eventerra\\": "src/Eventerra/"
        }
    },
    "authors": [
        {
            "name": "Igor Filippov",
            "email": "thisfil@ya.ru"
        }
    ]
}
